    "sequelize": "^5.12.3"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "3.16.0",
    "mocha": "3.2.0",
    "sequelize-cli": "^5.5.0",
    "sqlite3": "^4.0.9",
    "superagent": "3.5.2"
  }
